{
    "name": "alanmburr/blade-bootstrap-icons",
    "description": "A package to easily make use of Bootstrap Icons in your Laravel Blade views.",
    "keywords": [
        "Blade",
        "Bootstrap Icons",
        "Laravel"
    ],
    "homepage": "https://github.com/alanmburr/blade-bootstrap-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "David H. Sianturi",
            "email": "davidhsianturi@gmail.com",
            "homepage": "https://davidhsianturi.com",
            "role": "Developer"
        },
        {
            "name": "Alan M. Burr",
            "email": "info@alanburr.us.eu.org",
            "homepage": "https://alanburr.us.eu.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-icons": "^1.0",
        "twbs/bootstrap-icons": "^1.11.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AlanMBurr\\BladeBootstrapIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlanMBurr\\BladeBootstrapIcons\\BladeBootstrapIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
